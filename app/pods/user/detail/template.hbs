
{{#jf-page id='user-detail'}}
  {{#jf-page-title}}Users{{/jf-page-title}}
  {{#jf-page-bar}}
    {{#jf-page-actions}}
      {{#jf-page-action-button}}
          Actions {{fa-icon icon='angle-down'}}
      {{/jf-page-action-button}}
      {{#jf-page-action-list}}
        {{#jf-page-action}}
          {{#link-to 'user.new'}}Add New{{/link-to}}
        {{/jf-page-action}}
        {{#jf-page-action}}
          {{#link-to 'user.new'}}Change Status{{/link-to}}
        {{/jf-page-action}}
        {{#jf-page-action}}
          {{#link-to 'user.new'}}Set Role{{/link-to}}
        {{/jf-page-action}}
      {{/jf-page-action-list}}
    {{/jf-page-actions}}
  {{/jf-page-bar}}
  <!-- END PAGE HEADER-->
  <!-- BEGIN PAGE CONTENT-->
  {{#jf-page-content-container}}

    {{#jf-row}}
        <div class="col-md-4">
            <!-- PORTLET MAIN -->
            <div class="profile-sidebar">
                <!-- PORTLET MAIN -->
                <div class="portlet light profile-sidebar-portlet">
                    <!-- SIDEBAR USERPIC -->
                    <div class="profile-userpic">
                        <img src="img/ccmh/350x350.gif" class="img-responsive" alt="">
                    </div>
                    <!-- END SIDEBAR USERPIC -->
                    <!-- SIDEBAR USER TITLE -->
                    <div class="profile-usertitle">
                        <div class="profile-usertitle-name">
                            {{model.firstname}} {{model.lastname}}
                        </div>
                    </div>
                    <!-- END SIDEBAR USER TITLE -->
                    <!-- SIDEBAR BUTTONS -->
                    <div class="profile-userbuttons">
                        <button type="button" class="btn btn-circle btn-danger btn-sm">Message</button>
                    </div>
                    <!-- END SIDEBAR BUTTONS -->
                    <!-- SIDEBAR MENU -->
                    <div class="profile-usermenu">
                        <ul class="nav">
                            <li class="active">
                                <a href="extra_profile.html">
                                    <i class="icon-home"></i>
                                    Overview </a>
                            </li>
                            <li>
                                <a href="extra_profile_account.html">
                                    <i class="icon-settings"></i>
                                    Account Settings </a>
                            </li>
                        </ul>
                    </div>
                    <!-- END MENU -->
                </div>
                <!-- END PORTLET MAIN -->

            </div>
        </div>
        <!-- END PORTLET MAIN -->
        <!-- PORTLET MAIN -->
        <div class="col-md-8">
            <div class="panel panel-default">
                <!-- Default panel contents -->
                <div class="panel-heading">
                    <h3 class="panel-title">Details</h3>
                </div>
                <!-- List group -->
                <ul class="list-group">
                    <li class="list-group-item">Username <span class="pull-right">{{model.username}}</span></li>
                    <li class="list-group-item">Email <span class="pull-right">{{model.email}}</span></li>
                    <li class="list-group-item">Employee ID <span
                            class="pull-right">{{model.employee_id}}</span></li>
                    <li class="list-group-item">Status <span class="badge badge-success">{{model.status}}</span>
                    </li>
                    <li class="list-group-item">roles<span
                            class="pull-right">{{#each role in model.roles}}{{role.name}}, {{/each}}</span>
                    </li>
                    <li class="list-group-item">Departments<span class="pull-right">
                      {{#each department in model.departments}}
                        {{#link-to 'department.detail' department.id}}
                            <button type="button" class="btn btn-circle red-pink btn-xs">{{department.name}}</button>
                        {{/link-to}}
                      {{/each}}
                        </span>
                    </li>
                </ul>
            </div>
        </div>



