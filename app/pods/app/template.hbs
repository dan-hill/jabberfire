{{#jf-app-header}}
  {{#jf-app-logo}}
    {{#link-to 'dashboard'}}
      {{jf-img src="img/ccmh/header_logo.png" alt="logo" class="logo-default"}}
    {{/link-to}}
  {{/jf-app-logo}}
  {{#jf-app-toggler data-toggle='collapse' data-target='.navbar-collapse'}}{{/jf-app-toggler}}

  <!-- END RESPONSIVE MENU TOGGLER -->
  <!-- BEGIN TOP NAVIGATION MENU -->
  {{#jf-app-menu}}

    {{#jf-app-notifications}}
      {{#jf-dropdown-toggle}}
        {{sl-icon icon='bell'}}{{#jf-badge color='default'}}1{{/jf-badge}}
      {{/jf-dropdown-toggle}}
      {{#jf-dropdown-menu}}
        {{#jf-dropdown-menu-header}}
            <h3><span class="bold">1 pending</span> notifications</h3>
            <a href="extra_profile.html">view all</a>
        {{/jf-dropdown-menu-header}}
        {{#jf-dropdown-menu-scroller}}

          {{! ITEM }}
          {{#jf-dropdown-menu-scroller-item}}
            {{#jf-dropdown-menu-scroller-item-time}}
                just now
            {{/jf-dropdown-menu-scroller-item-time}}
            {{#jf-dropdown-menu-scroller-item-details}}
              {{#jf-dropdown-menu-scroller-item-label}}
                {{fa-icon icon='plus'}}
              {{/jf-dropdown-menu-scroller-item-label}}
              New user registered.
            {{/jf-dropdown-menu-scroller-item-details}}
          {{/jf-dropdown-menu-scroller-item}}
        {{! /ITEM }}

        {{/jf-dropdown-menu-scroller}}
      {{/jf-dropdown-menu}}

    {{/jf-app-notifications}}
    <!-- END NOTIFICATION DROPDOWN -->
    <!-- BEGIN INBOX DROPDOWN -->
    {{#jf-app-inbox}}
      {{#jf-dropdown-toggle}}
        {{sl-icon icon='envelope-open'}}{{#jf-badge color='default'}}1{{/jf-badge}}
      {{/jf-dropdown-toggle}}
      {{#jf-dropdown-menu}}
        {{#jf-dropdown-menu-header}}
            <h3>You have <span class="bold">1 new</span> message</h3>
            <a href="extra_profile.html">view all</a>
        {{/jf-dropdown-menu-header}}
        {{#jf-dropdown-menu-scroller}}
          {{#jf-dropdown-menu-scroller-item}}
           {{#jf-dropdown-menu-scroller-item-photo}}
             {{jf-img-circle src=''}}
           {{/jf-dropdown-menu-scroller-item-photo}}
            {{#jf-dropdown-menu-scroller-item-subject}}
              {{#jf-dropdown-menu-scroller-item-from}}
                Jimmy Dean
              {{/jf-dropdown-menu-scroller-item-from}}
            {{#jf-dropdown-menu-scroller-item-time}}
                just now
            {{/jf-dropdown-menu-scroller-item-time}}
            {{/jf-dropdown-menu-scroller-item-subject}}
            {{#jf-dropdown-menu-scroller-item-message}}
              I hear you are really into sausage.
            {{/jf-dropdown-menu-scroller-item-message}}
          {{/jf-dropdown-menu-scroller-item}}
        {{/jf-dropdown-menu-scroller}}
      {{/jf-dropdown-menu}}

    {{/jf-app-inbox}}
    <!-- END INBOX DROPDOWN -->
    <!-- BEGIN USER LOGIN DROPDOWN -->
    <!-- DOC: Apply "dropdown-dark" class after below "dropdown-extended" to change the dropdown styte -->
    <li class="dropdown dropdown-user">
        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown"
           data-close-others="true">
          {{fa-icon icon='user'}}
            <span class="username username-hide-on-mobile">
					{{session.currentUser.firstname}} </span>
            <i class="fa fa-angle-down"></i>
        </a>
        <ul class="dropdown-menu dropdown-menu-default">
            <li>
                <a href="">
                    <i class="icon-user"></i> My Profile </a>
            </li>
            <li>
                <a href="">
                    <i class="icon-settings"></i> Settings </a>
            </li>

            <li class="divider">
            </li>
            <li>
                <a href="extra_lock.html">
                    <i class="icon-lock"></i> Lock Screen </a>
            </li>
            <li>
                <a href="login.html">
                    <i class="icon-key"></i> Log Out </a>
            </li>
        </ul>
    </li>
    <!-- END USER LOGIN DROPDOWN -->
    <!-- BEGIN QUICK SIDEBAR TOGGLER -->
    <!-- DOC: Apply "dropdown-dark" class after below "dropdown-extended" to change the dropdown styte -->
    <li class="dropdown dropdown-quick-sidebar-toggler">
        <a href="javascript:;" class="dropdown-toggle">
            <i class="icon-logout"></i>
        </a>
    </li>
    <!-- END QUICK SIDEBAR TOGGLER -->
  {{/jf-app-menu}}
{{/jf-app-header}}

<!-- END HEADER -->
{{jf-clearfix}}

<!-- BEGIN CONTAINER -->
{{#jf-app-content-container}}
  {{#jf-sidebar-nav}}
    {{#jf-nav-item id='dashboard-nav'}}
        {{#link-to 'dashboard'}}
          {{sl-icon icon='home'}}
          <span class="title">Dashboard</span>
          <span class="selected"></span>
        {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='assets-nav'}}
      {{#link-to 'asset-list'}}
        {{sl-icon icon='home'}}
        <span class="title">Assets</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='manufacturers-nav'}}
      {{#link-to 'manufacturers'}}
        {{sl-icon icon='home'}}
        <span class="title">Manufacturers</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='Suppliers-nav'}}
      {{#link-to 'dashboard'}}
        {{sl-icon icon='home'}}
        <span class="title">Suppliers</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='user-nav'}}
      {{#link-to 'users'}}
        {{sl-icon icon='user'}}
        <span class="title">Users</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='Orders-nav'}}
      {{#link-to 'dashboard'}}
        {{sl-icon icon='home'}}
        <span class="title">Orders</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='Reports-nav'}}
      {{#link-to 'dashboard'}}
        {{sl-icon icon='home'}}
        <span class="title">Reports</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='Settings-nav'}}
      {{#link-to 'dashboard'}}
        {{sl-icon icon='home'}}
        <span class="title">Settings</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='department-nav'}}
      {{#link-to 'departments'}}
        {{sl-icon icon='layers'}}
        <span class="title">Departments</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}
    {{#jf-nav-item id='support-nav'}}
      {{#link-to 'support.manual'}}
        {{sl-icon icon='support'}}
        <span class="title">Support</span>
        <span class=""></span>
      {{/link-to}}
    {{/jf-nav-item}}

    {{/jf-sidebar-nav}}
    <!-- END SIDEBAR -->
    <!-- BEGIN CONTENT -->
    {{#jf-page-content}}
      {{outlet 'content'}}
    {{/jf-page-content}}


{{/jf-app-content-container}}
<!-- END CONTAINER -->
